{
    "name": "Pixelfed",
    "id": "pixelfed",
    "packaging_format": 1,
    "description": {
        "en": "ActivityPub Federated Image Sharing"
    },
    "version": "0.8.4~ynh1",
    "url": "https://pixelfed.org/",
    "license": "AGPL-3.0-or-later",
    "maintainer": {
        "name": "jibec",
        "email": "jean-baptiste@holcroft.fr"
    },
    "requirements": {
        "yunohost": ">= 3.0.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for Pixelfed"
                },
                "example": "domain.org"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public application?"
                },
                "default": true
            }
        ]
    }
}
